package tacos.data;

import org.springframework.data.repository.CrudRepository;

import tacos.Ingredient;

//defining Repository interfaces, same as the JDBCtemplate project
//Spring will generate implementations of our interface... 
//...as long as it extends one of Springs own repo-interfaces 

public interface IngredientRepository extends CrudRepository<Ingredient, String> {
	//CrudRepository interface define the method needed
	//so there is no need to explicitly defining them
	
}
